= form_for @selecionado do |f|
  - if @selecionado.errors.any?
    #error_explanation
      %h2= "#{pluralize(@selecionado.errors.count, "error")} prohibited this selecionado from being saved:"
      %ul
        - @selecionado.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :candidato, 'Candidato'
    = f.collection_select(:candidato_id, Candidato.all, :id, :nome, :prompt => true)
  .actions
    = f.submit 'Save'
